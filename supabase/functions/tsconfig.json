{
  "compilerOptions": {
    "lib": ["deno.ns", "deno.window"],
    "types": [
      "https://deno.land/std@0.190.0/http/server.d.ts",
      "https://esm.sh/stripe@16.2.0?target=deno&deno-std=0.190.0",
      "https://esm.sh/@supabase/supabase-js@2.45.0"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "target": "esnext",
    "module": "esnext"
  },
  "include": ["**/*.ts"]
}